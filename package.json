{
  "name": "dotnet-ng2-webpack",
  "version": "0.1.0",
  "description": "Sample project using angular2 compiled using webpack in a dotnet website",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dll": "webpack --config webpack.config.dll.js",
    "watch": "webpack --watch --progress",
    "build:prod": "cross-env APP_ENVIRONMENT=production webpack -p",
    "serve": "webpack-dev-server --inline --hot -d",
    "serve:prod": "cross-env APP_ENVIRONMENT=production webpack-dev-server --inline -p",
    "postinstall": "typings install"
